{
  "name": "haskind",
  "version": "0.5.3",
  "description": "Good ol' Haskell functions for use in Javascript",
  "main": "index.js",
  "scripts": {
    "deps": "npm install",
    "lint": "eslint ./{src,test}/**/*.js --fix",

    "clean:deps": "rm -rf node_modules",
    "clean:prod": "rm -rf dist",
    "clean": "run-p clean:prod",

    "build:prod": "NODE_ENV=production babel --out-dir=dist src",
    "build:dev": "# babel --out-dir=dist/Typed src",
    "build": "run-p build:*",

    "test:src": "NODE_PATH=$PWD/src mocha test/**/*.js",
    "test:prod": "NODE_PATH=$PWD/dist mocha test/**/*.js",
    "test:dev": "# NODE_PATH=$PWD/dist/Typed mocha test/**/*.js",
    "test": "run-p test:*",

    "repl": "hkci -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrRacoon/haskind.git"
  },
  "keywords": [
    "haskell",
    "functions",
    "hkci"
  ],
  "author": "Erik Sutherland <erik.misu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrRacoon/haskind/issues"
  },
  "homepage": "https://github.com/MrRacoon/haskind#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.11.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "should": "^11.1.1"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {}
      ]
    ],
    "plugins": [
      "transform-flow-strip-types",
      "transform-runtime",
      [
        "typecheck",
        {
          "disable": {
            "production": true
          }
        }
      ]
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.20.0"
  }
}
