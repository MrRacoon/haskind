{
  "name": "haskind",
  "version": "0.1.0",
  "description": "Good ol' Haskell functions for use in Javascript",
  "main": "index.js",
  "scripts": {
    "build": "run-s clean:dist deps compile",
    "clean": "run-p clean:*",
    "clean:deps": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "compile": "babel --out-dir=dist src",
    "deps": "npm install",
    "full": "run-s build test",
    "lint": "eslint ./{src,test}/**/*.js --fix",
    "mocha": "mocha test/**/*.js",
    "test": "run-s lint mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrRacoon/haskind.git"
  },
  "keywords": [
    "haskell",
    "functions"
  ],
  "author": "Erik Sutherland <erik.misu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrRacoon/haskind/issues"
  },
  "homepage": "https://github.com/MrRacoon/haskind#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.11.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "should": "^11.1.1"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {}
      ]
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  }
}
